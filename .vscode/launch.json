    {
      "type": "node",
      "request": "launch",
      "name": "Server Start (NODE_ENV=production)",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/server/index.js",
      "env": {
        "NODE_ENV": "production"
      },
      "cwd": "${workspaceFolder}/server"
    }
  ]
